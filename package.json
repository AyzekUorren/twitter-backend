{
  "name": "twitter-app-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "12.10.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/main.js",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "start:dev": "nodemon",
    "start:develop": "cross-env NODE_ENV=DEV ts-node -r tsconfig-paths/register src/main.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "cross-env NODE_ENV=DEV jest",
    "test:watch": "cross-env NODE_ENV=DEV jest --watch",
    "test:cov": "cross-env NODE_ENV=DEV jest --coverage",
    "test:debug": "cross-env NODE_ENV=DEV node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=DEV jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.6.7",
    "@nestjs/core": "^6.6.7",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/microservices": "^6.6.7",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.6.7",
    "@nestjs/swagger": "^3.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "dotenv": "^7.0.0",
    "mongoose": "^5.7.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.7.1",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.6.7",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^23.3.13",
    "@types/mongoose": "^5.5.17",
    "@types/node": "^10.14.18",
    "@types/passport-jwt": "^3.0.2",
    "@types/supertest": "^2.0.8",
    "cross-env": "^5.2.1",
    "husky": "^2.7.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.2.1",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --config .prettierrc",
      "git add"
    ]
  }
}
